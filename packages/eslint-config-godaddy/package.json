    "javascript",
    "styleguide",
    "style-guide",
    "eslint",
    "es6"
  ],
  "bin": {
    "eslint-godaddy": "bin/eslint-godaddy"
  },
  "peerDependencies": {
    "eslint": ">=6.0.0"
  },
  "dependencies": {
    "eslint-plugin-json": "*",
    "eslint-plugin-mocha": "*",
    "which": "^1.2.12"
  },
  "devDependencies": {
    "eslint": "^6.1.0",
    "eslint-find-rules": "^3.4.0"
  }
}
