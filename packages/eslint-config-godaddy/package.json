{
  "name": "eslint-config-godaddy",
  "version": "4.0.0",
  "description": "Base ESLint config for consistent style in ES6 projects at GoDaddy.",
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run --silent lint",
    "test": "echo ok",
    "unused": "eslint-find-rules --unused ./index.js || echo ''"
